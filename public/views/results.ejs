<% include public/views/header.ejs %>

<div class="grid-container">
    <div class="grid-x">
        <div class="cell small-12">
            <h3>NNSP Sign-In</h3>
        </div>
    </div>
    <div class="grid-x ">
        <div class="cell small-6 large-6"><h4 id="date"></h4></div>
        <div class="cell small-5 large-5"><h4 id="weekDay"></h4></div>
        <div class="cell small-1 large-1"><h4 id="dayNight"></h4></div>
    </div>
    <hr>
    <div class="grid-x">
        <h5>Team 1</h5>
    </div>
    <div class="grid-x">
        <div class="cell small-1">
            <h6><strong>ID</strong></h6>
        </div>
        <div class="cell small-2">
            <h6><strong>Name</strong></h6>
        </div>
        <div class="cell small-2">
            <h6><strong>Rating</strong></h6>
        </div>
        <div class="cell small-2">
            <h6><strong>Time</strong></h6>
        </div>
        <div class="cell small-2">
            <h6><strong>Radio</strong></h6>
        </div>
        <div class="cell small-1">
            <h6><strong>Days</strong></h6>
        </div>
        <div class="cell small-2">
            <h6><strong>Guest</strong></h6>
        </div>
    </div>
    <div class="grid-x">
        <% data.forEach((result) => { %>
            <% if (Number(result.TEAM) === 1) { %>
                <div class="cell small-1">
                    <%= result.ID %>
                </div>
                <div class="cell small-2">
                    <%= result.NAME %>
                </div>
                <div class="cell small-2">
                    <%= result.RATING %>
                </div>
                <div class="cell small-2">
                    <%= result.TIME %>
                </div>
                <div class="cell small-2">
                    <%= result.RADIO %>
                </div>
                <div class="cell small-1">
                    <%= result.DAYS %>
                </div>
                <div class="cell small-2">
